{
  "name": "parcel-plugin-imagemin",
  "version": "2.1.3",
  "description": "Parcel plugin to support image minification",
  "main": "./lib/index.js",
  "license": "MIT",
  "author": "Jasper De Moor",
  "scripts": {
    "test": "mocha --timeout 50000",
    "format": "prettier --write \"./{lib,test}/**/*.{js,json,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/DeMoorJasper/parcel-plugin-imagemin.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,mdx}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "parcel",
    "image",
    "minification"
  ],
  "dependencies": {
    "fs-extra": "^7.0.0",
    "imagemin": "^6.1.0",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^6.0.0",
    "imagemin-webp": "^4.1.0",
    "parcel-bundler": "^1.10.3"
  },
  "devDependencies": {
    "husky": "^2.4.1",
    "lint-staged": "^8.1.3",
    "mocha": "^5.1.1",
    "parcel-assert-bundle-tree": "^1.0.0",
    "prettier": "^1.16.4"
  }
}
